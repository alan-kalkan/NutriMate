{
  "expo": {
    "name": "Nutrimate: ",
    "displayName": "Nutrimate: ",
    "slug": "client",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alankalkan.client"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "79d5985c-9ed9-43a0-a81d-1d0bd67c7da0"
      },
      "ios": {
        "bundleIdentifier": "com.alankalkan.client"
      }
    },
    "android": {
      "package": "com.alankalkan.client"
    }
  }
}
